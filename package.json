{
  "name": "maidBot",
  "version": "1.0.0",
  "description": "",
  "main": "server/server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "npm install && npm run server",
    "start:dev": "concurrently  \"nodemon --ignore 'client/*'\" \"npm run server\"  \"npm run client\" ",
    "server": "cd server && npm install",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dashinja/maid-bot.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dashinja/maid-bot/issues"
  },
  "homepage": "https://github.com/dashinja/maid-bot#readme",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7"
  },
  "dependencies": {
    "express": "^4.16.3",
    "if-env": "^1.0.4"
  }
}
